<!DOCTYPE html>
<html>
<head >

    <partial name="_MetaTags" />
    <partial name="_Links" />

    <title>@ViewBag.Title | AppliancesShop</title>

    <script src="~/AdminTheme/assets/js/modernizr.min.js"></script>

</head>

<body class="fixed-left">

    <div id="wrapper">

        <partial name="_Wrapper" />
        
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class=" container">

                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>

            </div> <!-- content -->

            <footer class="footer text-right">
                2015 © Moltran.
            </footer>

        </div>

        <div id="MainModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
            <div class="modal-dialog">
                <div class="modal-content" id="ModalContent">
                </div>
            </div>
        </div>

    </div>

    <partial name="_Scripts" />

    @RenderSection("Scripts",false);

</body>
</html>
